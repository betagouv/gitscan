{
  "name": "@proconnect-gouv/proconnect.testing",
  "version": "1.0.4",
  "homepage": "https://github.com/proconnect-gouv/proconnect-identite/tree/main/packages/testing#readme",
  "bugs": "https://github.com/proconnect-gouv/proconnect-identite/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proconnect-gouv/proconnect-identite.git",
    "directory": "packages/testing"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#src/*": "./src/*/index.ts"
  },
  "exports": {
    "./*": {
      "require": "./dist/*/index.js",
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.lib.json && node dist/scripts/copy-json.js",
    "dev:test": "tsx --watch --test src/**/*.test.ts tests/**/*.test.ts",
    "test": "npm run test:unit",
    "test:unit": "tsx --test --test-reporter=spec src/**/*.test.ts tests/**/*.test.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "@hono/zod-validator": "^0.7.2",
    "@proconnect-gouv/proconnect.api_entreprise": "1.1.1",
    "hono": "^4.11.10"
  },
  "devDependencies": {
    "@proconnect-gouv/proconnect.devtools.typescript": "1.0.0",
    "@types/oidc-provider": "^9.5.0",
    "@types/yargs": "^17.0.33",
    "yargs": "^18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
